@model IEnumerable<KeyPairModel>

@{
    ViewData["Title"] = "Privacy Policy";
}
<h1>@ViewData["Title"]</h1>

<div><b>Table Format Data Displaying in ForEach</b><br /></div>  
<table class="table table-bordered table-responsive table-hover">  
    <tr>  
        <th>Key Name </th>  
        <th>Key Description </th>  
        <th>Key created </th>  
        <th>Key expires</th>  
    </tr>  
    @foreach (var d in Model)  
    {  
        <tr>  
            <td>@d.KeyName</td>  
            <td>@d.KeyDescription</td>  
            <td>@d.Created</td>  
            <td>@d.Expires</td>  
            <td> <input type="button" value="Create" onclick="location.href='@Url.Action("DownloadPrivateKey", "Home")'" /></td>
        </tr>  
    }  
</table>  
@using (Html.BeginForm("AddToCart", "Controller", new { id = 0 }, FormMethod.Post))
{
  <input type="number" id="itemCount" name="itemCount"/>
  <button type="submit"><span>Add</span></button>
}
